<div id="sidecolumn">
	<div id="sidenav" class="blockbackground">
	</div>
</div>
<script id="sidemenuscript">
	$.getJSON("../children.json", function(json) {
		$("#sidenav").append('\
			<h2>'+json[0]+'</h2>\
			<ul id="sidenavlist">\
			</ul>\
		');
		json[1].forEach(function(child){
			$("#sidenavlist").append('\
				<li>\
					<a href="../'+child.directory+'">'+child.page+'</a>\
				</li>\
			')
		});
	});
</script>